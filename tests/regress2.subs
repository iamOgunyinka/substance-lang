class Foo {
	var a; var b;
	var c; var d;
	
	construct new(w) {
		a = w;
	}
	
	function a(x) : public {
		b = x;
		return self;
	}
	
	function b(y) : public {
		c = y;
		return self;
	}
	
	function c(z) : public {
		d = z;
		return a + b + c + d;
	}
